    <style>
        ::-webkit-scrollbar {
            width: 12px;
        }

        ::-webkit-scrollbar-thumb {
            background-color: #c1c1c1; /* Change this to the desired color for the thumb */
            border-radius: 6px;
            border: 3px solid #f1f1f1; /* Change this to the desired color for the border */
        }

            ::-webkit-scrollbar-thumb:hover {
                background-color: #a1a1a1; /* Change this to the desired color for the thumb on hover */
            }

        ::-webkit-scrollbar-track {
            background: transparent !important; /* Set the track background color to transparent */
        }
    </style>
</head>
