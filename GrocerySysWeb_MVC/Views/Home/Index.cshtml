    <header id="myHeader" class="">
        <img id="logo" src="~/Content/img/faitlogo.png" style="color: #fff;">



        <nav>
            <a href="#myHeader">Home</a>
            <a href="#categories">Categories</a>
            <a href="#aboutus">About Us</a>
            <a href="#future"><i class="fa fa-solid fa-magnifying-glass"></i></a>
            <a href="/Login/logout"><i class="fa-solid fa-user"></i></a>
            <button id="openmenu">
                <span></span>
                <span></span>
            </button>
        </nav>
